   "metadata": {},
   "source": [
    "@Entity\n",
    "@Table(name=\"clients\")\n",
    "@Data\n",
    "@NoArgsConstructor\n",
    "public class Client {\n",
    "    \n",
    "    @Id\n",
    "    @GeneratedValue(strategy = GenerationType.IDENTITY)\n",
    "\n",
    "    Long id;\n",
    "    String name;\n",
    "    String lastname;\n",
    "    Integer age;\n",
    "\n",
    "    public Client(String name, String lastname, Integer age) {\n",
    "        this.name = name;\n",
    "        this.lastname = lastname;\n",
    "        this.age = age;\n",
    "    }\n",
    "}"
   ]
  },
